{
  "name": "flexcomply",
  "version": "0.6.0",
  "scripts": {
    "client": "cd frontend && yarn install && yarn start",
    "client_build": "cd frontend &&  yarn install && yarn build",
    "server": "nodemon server.js",
    "start:local": "concurrently --kill-others-on-fail \"yarn server\" \"REACT_APP_API_URL=\"\" yarn client\"",
    "start:api": "concurrently --kill-others-on-fail \"REACT_APP_API_URL=\"https://google.com\" yarn client\"",
    "build": "concurrently --kill-others-on-fail \"REACT_APP_API_URL=\"https://google.com\" yarn client_build\"",
    "eslint": "eslint ."
  },
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}

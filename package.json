{
  "name": "flexcomply",
  "version": "0.6.0",
  "scripts": {
    "client": "cd frontend && yarn install && yarn start",
    "client_build": "cd frontend &&  yarn install && yarn build",
    "server": "nodemon server.js",
    "start:local": "concurrently --kill-others-on-fail \"yarn server\" \"REACT_APP_API_URL=\"\" yarn client\"",
    "start:api": "concurrently --kill-others-on-fail \"REACT_APP_API_URL=\"https://google.com\" yarn client\"",
    "build": "concurrently --kill-others-on-fail \"REACT_APP_API_URL=\"http://flexapi.nanoapp.io/api\" yarn client_build\"",
    "build:mock": "concurrently --kill-others-on-fail \"REACT_APP_API_URL=\"https://devel-server.com\" yarn client_build\"",
    "eslint": "eslint .",
    "test": "ls"
  },
  "dependencies": {
    "express": "^4.16.2",
    "immutable": "^4.0.0-rc.12",
    "redux-duck": "^1.0.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
